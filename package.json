{
  "name": "ngnx-data-proxy-database",
  "version": "1.0.8",
  "description": "A base class for persisting data to a database. This supports file system data stores as well, but is restricted to Node.js (backend) use. This will not work in a browser.",
  "main": "index.js",
  "scripts": {
    "test": "tape test/**/*.js | tap-spec && standard | snazzy --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ngnjs/ngnx-data-proxy-database.git"
  },
  "keywords": [
    "ngnx",
    "data",
    "proxy",
    "database"
  ],
  "author": "Corey Butler",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/ngnjs/ngnx-data-proxy-database/issues"
  },
  "homepage": "https://github.com/ngnjs/ngnx-data-proxy-database#readme",
  "dependencies": {
    "ngn-data": "^0.1.5"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "fs-extra": "^2.1.2",
    "gulp": "^3.9.1",
    "gulp-header": "^1.8.8",
    "gulp-strip-comments": "^2.4.5",
    "ngn": "^0.3.30",
    "shortbus": "^1.2.7",
    "snazzy": "^7.0.0",
    "standard": "^10.0.2",
    "standard-tap": "^1.0.1",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.3"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "_*",
      "_**/*"
    ],
    "globals": [
      "NGN",
      "ngn",
      "NGNX",
      "ngnx",
      "LoggingMethodError",
      "DuplicateLoggingMethodError"
    ]
  }
}
